    "def solution(brown, yellow):\n",
    "    y = 2\n",
    "    while True:\n",
    "        if 2*y*y - y*(brown+4) + 2*(brown+yellow) == 0:\n",
    "            x = (brown+4)/2 - y\n",
    "            answer = [int(x),y]\n",
    "            break\n",
    "        else:\n",
    "            y += 1\n",
    "    return answer"
   ]
