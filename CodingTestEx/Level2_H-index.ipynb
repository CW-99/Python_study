    "def solution(n):\n",
    "    import numpy as np\n",
    "    np_n = np.array(n)\n",
    "    lst = []\n",
    "    for i in range(0, len(n)+1):\n",
    "        if len(np_n[np_n > i]) == i:\n",
    "            lst.append(i)\n",
    "    return lst[-1]"
   ]
