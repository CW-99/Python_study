    "def solution(arr):\n",
    "    import math\n",
    "    mul = arr[0]\n",
    "    for i in range(1, len(arr)):\n",
    "        gcd = math.gcd(mul,arr[i])\n",
    "        mul = mul*arr[i] // gcd # 어차피 나누어 떨어지기 때문에 상관없다\n",
    "    return mul"
   ]
  }
