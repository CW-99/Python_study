    "def solution(n):\n",
    "    answer, x, = 1, 1\n",
    "    while True:\n",
    "        if x*(x+1)/2 <= n < (x+1)*(x+2)/2 :\n",
    "            break\n",
    "        x += 1\n",
    "    k = 1 \n",
    "    for i in range(2,x+1):\n",
    "        if (n-k)% i == 0:\n",
    "            answer += 1\n",
    "        k = k + i\n",
    "    return answer"
   ]
  }
