   "source": [
    "def solution(numbers, target):\n",
    "    import itertools as it\n",
    "    sum_num = sum(numbers)\n",
    "    answer = 0\n",
    "    for i in range(len(numbers)):\n",
    "        for subset in it.combinations(numbers, i):\n",
    "            if sum_num - 2* sum(subset) == target:\n",
    "                answer += 1\n",
    "    return answer"
   ]
  }
