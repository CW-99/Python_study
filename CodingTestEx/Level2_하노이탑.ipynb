    "    def solution2(n,f,m,t):\n",
    "        if n == 1:\n",
    "            return [[f,t]]\n",
    "        else:\n",
    "            answer = solution2(n-1,f,t,m) + [[f,t]] + solution2(n-1,m,f,t)\n",
    "        return answer\n",
    "    return solution2(n,1,2,3)"
